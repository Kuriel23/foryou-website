generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type GuildsCrimeschedule {
  id       String   @map("_id")
  schedule DateTime @db.Date
}

type GuildsDailyschedule {
  id       String   @map("_id")
  schedule DateTime @db.Date
}

type GuildsRepschedule {
  id       String   @map("_id")
  schedule DateTime @db.Date
}

type GuildsRobschedule {
  id       String   @map("_id")
  schedule DateTime @db.Date
}

type UsersConfig {
  notificarcrime     Boolean?
  notificardaily     Boolean?
  notificarep        Boolean?
  notificarob        Boolean?
  notificarwork      Boolean?
  protecaoraspadinha String?
  protecaoroleta     String?
}

type UsersLoja {
  akaza         String?
  akazoficial   String?
  arcane        String?
  bartsimpson   String?
  bellcraner    String
  cidadenoite   String
  cybergirl     String?
  douma1        String?
  floresta      String?
  garotamascara String
  gokublack     String
  hantengu      String?
  kanna         String
  keitaro       String?
  koalas        String?
  makisan       String?
  megumin       String
  nakiriayame   String
  nossoamor     String?
  raiden        String?
  rengoku       String?
  saokirito     String?
  satorugojo    String
  shinobu       String
  space         String?
  supersaiyajin String?
  tanjironezuko String
}

type UsersMedalhas {
  natalv1 Boolean
}

type UsersPunishments {
  autobot String[]
  mutes   String[]
}

model baninfos {
  id     String   @id @map("_id")
  v      Int      @map("__v")
  autor  String
  banido String
  data   String
  motivo String
  provas String[]
}

model guilds {
  id              String                @id @map("_id")
  v               Int                   @map("__v")
  crimeschedule   GuildsCrimeschedule[]
  dailyschedule   GuildsDailyschedule[]
  diadocalendario String
  lancadata       String
  lancahdata      String
  manutencao      Boolean
  newsdata        String
  notifylanca     Boolean
  repschedule     GuildsRepschedule[]
  robschedule     GuildsRobschedule[]
  /// Could not determine type: the field only had null or empty values in the sample set.
  workschedule    Int?
  ytdata          String
}

model halloweens {
  id    String @id @map("_id")
  v     Int    @map("__v")
  candy Int
}

model levels {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  v           Int      @map("__v")
  guildID     String
  lastUpdated DateTime @db.Date
  level       Int
  userID      String
  xp          Int
}

model users {
  id                String           @id @map("_id")
  v                 Int              @map("__v")
  /// Multiple data types found: Float: 9.5%, Int: 90.5% out of 599 sampled entries
  animecoins        Int
  biografia         String
  casado            String[]
  /// Multiple data types found: Float: 17.1%, Int: 82.9% out of 345 sampled entries
  clonaCooldown     Int?
  config            UsersConfig
  cor               String?
  coratm            String?
  crimeCooldown     Int?
  dailyCooldown     Int
  dinsujo           Int?
  equipado          String
  loja              UsersLoja
  medalhas          UsersMedalhas?
  punishments       UsersPunishments
  rep               Int
  repCooldown       Int
  roubarCooldown    Int
  trabalharCooldown Int
}
